<?xml version="1.0" encoding="utf-8"?><!--
   Sample data extraction rules file; uncomment and customize as necessary.
   See https://developer.android.com/about/versions/12/backup-restore#xml-changes
   for details.
-->
<data-extraction-rules>
    <cloud-backup>
        <exclude
            domain="file"
            path="./exoplayer" />
        <exclude
            domain="file"
            path="./download" />
        <exclude
            domain="database"
            path="exoplayer_internal.db" />
    </cloud-backup>
    <device-transfer>
        <exclude
            domain="file"
            path="./exoplayer" />
        <exclude
            domain="file"
            path="./download" />
        <exclude
            domain="database"
            path="exoplayer_internal.db" />
    </device-transfer>
</data-extraction-rules>